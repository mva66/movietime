":92,"column":16},"source":"components/login-view/login-view.jsx","original":{"line":35,"column":6}},{"generated":{"line":92,"column":17},"source":"components/login-view/login-view.jsx","original":{"line":31,"column":4}},{"generated":{"line":92,"column":18},"source":"components/login-view/login-view.jsx","original":{"line":30,"column":2}},{"generated":{"line":93,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":1}},{"generated":{"line":94,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":68,"column":0}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from \"react\";\r\nimport PropTypes from \"prop-types\";\r\nimport Container from \"react-bootstrap/Container\";\r\nimport Button from \"react-bootstrap/Button\";\r\nimport Form from \"react-bootstrap/Form\";\r\nimport axios from \"axios\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nexport function LoginView(props) {\r\n  const [username, setUsername] = useState(\"\");\r\n  const [password, setPassword] = useState(\"\");\r\n  const register = \"true\";\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    axios\r\n      .post(\"https://myflix16.herokuapp.com/login\", {\r\n        Username: username,\r\n        Password: password,\r\n      })\r\n      .then((response) => {\r\n        const data = response.data;\r\n        props.onLoggedIn(data);\r\n      })\r\n      .catch((e) => {\r\n        console.log(\"no such user\");\r\n      });\r\n  };\r\n\r\n  return (\r\n    <Container className=\"loginContainer\">\r\n      <h1>Welcome to My Flix</h1>\r\n      <br></br>\r\n      <br></br>\r\n      <Form>\r\n        <Form.Group controlId=\"formBasicUsername\">\r\n          <Form.Label>Username</Form.Label>\r\n          <Form.Control\r\n            type=\"text\"\r\n            placeholder=\"Enter username\"\r\n            value={username}\r\n            onChange={(e) => setUsername(e.target.value)}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group controlId=\"formBasicPassword\">\r\n          <Form.Label>Password</Form.Label>\r\n          <Form.Control\r\n            type=\"password\"\r\n            placeholder=\"Password\"\r\n            value={password}\r\n            onChange={(e) => setPassword(e.target.value)}\r\n          />\r\n        </Form.Group>\r\n        <Button id=\"loginButton\" onClick={handleSubmit}>\r\n          Log in\r\n        </Button>\r\n        <Link to={`/register`}>\r\n          <Button variant=\"link\" className=\"registerButton\" type=\"submit\">\r\n            Sign Up\r\n          </Button>\r\n        </Link>\r\n      </Form>\r\n    </Container>\r\n  );\r\n}\r\n\r\n/*LoginView.propTypes = {\r\n  onLoggedIn: PropTypes.func.isRequired,\r\n};*/\r\n"},"lineCount":null}},"error":null,"hash":"1b0853c5f76727c8ca3e65309f2bd933","cacheData":{"env":{}}}